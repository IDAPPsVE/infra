 <% include ../views/partials/dashHeader %>
  
  <article class="col-md-8 maincontent">
				<header class="page-header">
					<h1 class="page-title">Registrar solvencia</h1>
				</header>
      <% if (message.length > 0) { %>
          <div class="bg-info"><%= message %></div>
      <% } %>

      <form action="/MaraBox/admin/registroSolvencia" method="post">
        
          <div class="form-group">
              <label>Cedula</label>
              <input type="text" class="form-control" name="cedula">
          </div>
          <div class="form-group">
              <label>Fecha de Pago</label>
              <input type="text" class="form-control" id="fecha" name="fechaInicio">
          </div>
          <div class="form-group">
              <label>Fecha Proximo pago</label>
              <input type="text" class="form-control" id="fecha" name="fechaCulminacion">
          </div>
          
          <button type="submit" class="btn btn-primary">Registrar Solvencia</button>
      </form>

      </article>
			<!-- /Article -->

		</div>
	</div>	<!-- /container -->

  <footer>
    <% include ../views/partials/dashFooter %>
  </footer>
</body>
</html>
